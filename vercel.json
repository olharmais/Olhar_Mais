{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/nutricionista/(.*)",
      "dest": "/nutricionista/$1"
    },
    {
      "src": "/profissional/(.*)",
      "dest": "/profissional/$1"
    },
    {
      "src": "/responsavel/(.*)",
      "dest": "/responsavel/$1"
    },
    {
      "src": "/gestor/(.*)",
      "dest": "/gestor/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 